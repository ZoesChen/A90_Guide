WORKDIR = `pwd`

CC = mips-linux-gnu-gcc
CXX = mips-linux-gnu-g++
AR = mips-linux-gnu-ar
LD = mips-linux-gnu-g++

CFLAGS = -Wall -fexceptions -O2 -fPIC

OUT_FILE := update_uboot
OBJ_FILE := main.o param.o uboot.o

all:  $(OBJ_FILE)
	$(LD) -o $(OUT_FILE) $(OBJ_FILE)

main.o: main.cpp
	$(CXX) $(CFLAGS)  -c main.cpp -o main.o

param.o: param.cpp
	$(CXX) $(CFLAGS)  -c param.cpp -o param.o

uboot.o: uboot.cpp
	$(CXX) $(CFLAGS)  -c uboot.cpp -o uboot.o

.PHONY:clean
clean:
	rm  $(OUT_FILE) *.o

